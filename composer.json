  "license": "Apache-2.0",
  "keywords": [
    "API",
    "Amazon",
    "PHP",
    "MWS",
    "Laravel"
  ],
  "authors": [
    {
      "name": "Rigel Kent Carbonel",
      "email": "rigel20.kent@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0|^7.2|^7.3|^7.4",
    "ext-curl": "*",
    "illuminate/support": "5.*|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0|^6.0|^8.0"
  },
  "autoload": {
    "psr-0": {
      "Luigel\\AmazonMws\\": "src"
    }
  },
  "minimum-stability": "stable"
